#define _CRT_SECURE_NO_WARNINGS 1
//找出100-200之间的素数
#include<stdio.h>
int main()
{
	int i;
	int count=0;
	for (i = 100; i < 201; i++)
	{
		int a;
		for ( a = 2; a < i; a++)
		{
			if (i % a == 0)
			{
				break;
			}
		
		}
	    if (a == i)
	    {
	      printf("%d ", i);
		  count++;
	     }		
	}
	printf("\ncount=%d\n", count);
	return 0;
}
