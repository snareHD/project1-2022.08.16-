#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
//打印100个大家好
int main()
{
	int i = 0;
	for (; i < 10; i++)
	{
		int j = 0;
		for (; j < 10; j++)
		{
			printf("大家好\n");
		}
	}
	return 0;
}

//只能打印10个大家好
int main()
{
	int i = 0;
	int j = 0;
	for (; i < 10; i++)
	{
		
		for (; j < 10; j++)
		{
			printf("大家好\n");
		}
	}
	return 0;
}
