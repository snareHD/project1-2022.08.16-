#define _CRT_SECURE_NO_WARNINGS 1

//求两个值之间的最大公约数(用辗转相除法)。
#include<stdio.h>
int main()
{
	int a = 24;
	int b = 18;
        int i = 0;
	scanf("%d%d", &a, &b);
	if (a < b)
	{
		int z = a;
		a = b;
		b = z;
	}

	while ((a % b) != 0)
	{
	        i = a % b;
		a = b;
		b = i;
	}

		
		printf("%d\n", b);
	
	return 0;
}
